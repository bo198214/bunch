@Article{ref0728.06006,
author="Griggs, J.R.",
title="Iterated exponentials of two numbers",
journal="Discrete Math.",
year="1991",
volume="88",
number="2-3",
pages="193--209",
optkeywords="iterated exponentials",
optkeywords="tower order",
abstract="Let $x\sb i$ $(i=1,2,...,n)$ be reals, $0\le x\sb i$ for each $x\sb i$. Denote $$ T(x\sb1 x\sb2 \dots x\sb n) = \left\vert \matrix &amp;&amp;&amp;x\sb n \ &amp;&amp;\vdots&amp; \ &amp;x\sb2&amp;&amp; \ x\sb1&amp;&amp;&amp; \endmatrix\right. $$ Next let c be a real, $c\ge 0$; let $A\sb n(c)$ be the system of all words of length n in a and b. If we consider the variables a and b as reals, $a\ge 0$ and $b\ge 0$, then for $w\in A\sb n(c)$ the symbol T(w) has the obvious meaning. We define a relation $\le$ in $A\sb n(c)$ as follows: For w and $w{\textquoteright}$ in $A\sb n(c)$ we put $w\le w{\textquoteright}$ if, whenever $b\ge a\ge c$, then $T(w)\le T(w{\textquoteright})$. The relation $\le$ is a partial order in $A\sb n(c)$. In the paper there are given complete descriptions of $A\sb n(c)$ for the following cases: (i) $c=e$ and n arbitrary; (ii) $c=3.6$ and n arbitrary; (iii) $n\le 3$ and $c\ge 0$. Some conjectures on $A\sb n(c)$ are proposed.",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=255), last updated on Sun, 18 Apr 2010 23:33:02 +0200",
doi="10.1016/0012-365X(91)90009-Q"
}

