@Article{Viana:hairs,
author="Viana da Silva, M.",
title="The differentiability of the hairs of exp(Z)",
journal="Proc. Am. Math. Soc.",
year="1988",
volume="103",
number="4",
pages="1179--1184",
optkeywords="iterate",
optkeywords="itinerary",
optkeywords="hair",
abstract="In studying the dynamics of the map $z\to $\lambda$ e\sp z$, where $$\lambda$ =$\rho$ e\sp{i\vartheta}$, $-$\pi$ \le \vartheta &lt;$\pi$$, $$\rho$ &gt;0$, {\it R. L. Devaney} and {\it M. Krych}, Ergodic Theory Dyn. Syst. 4, 35-52 (1984; Zbl 0567.58025), introduced the itinerary $S=(S\sb n)$ of a point $z:S\sb n$, $n=0,1,2,...$, is the integer such that $$ 2(S\sb n-1)$\pi$ \le Im f\sp n(z)+\vartheta &lt;2(S\sb n+1)$\pi$. $$ If S is the f-itinerary of some point z then there is a real $\hat x=\vert z\vert +2$\pi$$ such that $(2(S\sb n)+1)$\pi$ +\vert \vartheta \vert \le g\sp n(\hat x)$, where $g(z)=$\rho$ e\sp n$. The tail of the hair of f associated to s is $T(s)=\{z:$ z has itinerary s and for all n, Re $f\sp n(z)\ge g\sp n(\hat x)\}$. Let $s\sp k$ denote the k-th shift of s. Then the hair of f associated with s is $C(s)=\cup\sp{\infty}\sb{k=0}C\sb k(s)$, where $C\sb k(s)$ is the connected component of $f\sp{-k}(T(s\sp k))$ which contains T. The author constructs a parametrization $\beta$ : [\^x,$\infty)\to T$ which is $C\sp{\infty}$ and thus shows that the hair C is a smooth curve.",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=233), last updated on Tue, 30 Mar 2010 20:22:00 +0200",
opturl="http://eretrandre.org/rb/files/VianadaSilva1988_233.pdf",
