@Article{Walker:solutions,
author="Walker, Peter L.",
title="On the solutions of an {A}belian functional equation",
journal="J. Math. Anal. Appl.",
year="1991",
volume="155",
number="1",
pages="93--110",
optkeywords="abel",
optkeywords="analytic",
optkeywords="equation",
optkeywords="file-import-09-03-19",
optkeywords="iterations",
optkeywords="solutions",
abstract="The paper is principally concerned with solutions of the Abel equation $(*)\quad f(e\sp{z/e})=f(z)+1.$ Studying iterations of $1-e\sp{-z}$ and $\log (1+z)$ at zero two distinct solutions of (*) are obtained, which are analytic on domains which include the intervals (-$\infty,e)$ and $(e,+\infty)$, respectively. \par Actually, the first one is analytic and satisfies (*) in particular for Re z$&lt;e$ and has period $2\pi$ ie, whereas the second is analytic on ${\bbfC}\setminus (-\infty,e]$ and satisfies (*) for $z\in {\bbfC}\setminus (-\infty,e]$ such that $\vert Im z\vert &lt;\pi e$. Numerical values of those functions for some real and complex values of the argument are also determined.[K.Baron]",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=109), last updated on Tue, 17 Nov 2009 22:26:13 +0100",
doi="10.1016/0022-247X(91)90029-Y",
opturl="http://www.zentralblatt-math.org/zmath/en/advanced/?q=an:0716.39006",
opturl="http://eretrandre.org/rb/files/Walker1991_109.pdf",
opturl=""
}

