@Article{ref47.0921.02,
author="Fatou, P.",
title="Sur les {\'e}quations fonctionnelles",
journal="S. M. F. Bull.",
year="1920",
volume="47",
pages="161--271; 48, 33",
abstract="In der vorliegenden Arbeit studiert Verf. haupts{\"a}chlich das Verhalten (im Kleinen und im Gro{\ss} en) der Folge der Iterierten einer rationalen Funktion $R(z)$ der komplexen Variabeln $z$, d. h. der Folge $R_1(z) = R(z), R_2(z) = R(R(z)), \dots, R_n(z) = R(R_{n-1}(z)) = R_{n-1}(R(z)), \dots.$ Die hier auftretenden Fragen h{\"a}ngen nahe mit der Theorie der Funktionalgleichungen, insbesondere der Schr{\"o}derschen und der Abelschen Funktionalgleichung und verwandter Gleichungen zusammen. Die Arbeit hat in Problemstellungen, Methoden und Ergebnissen mit den etwas fr{\"u}heren Untersuchungen von G. Julia (vgl. F. d. M. 46, 520 (JFM 46.0520.*), 1916-18) vieles gemein, doch vermag Verf. noch weitergehende Resultate zu gewinnen. \par Zun{\"a}chst sei die Erkl{\"a}rung einiger Bezeichnungen vorausgeschickt. Ist $z_n = R_n(z_0),$ so hei{\ss} t $z_n$ Nachfolger $n$-ter Ordnung von $z_0,$ umgekehrt hei{\ss} t $z_0.$ Vorg{\"a}nger $n$-ter Ordnung von $z_n.$ Ist $$\zeta$ = R_n($\zeta$),$ so ist auch $R($\zeta$) = R_n(R($\zeta$)),$ d. h. der Nachfolger eines Fixpunktes ist wieder ein Fixpunkt. Ein Fixpunkt $\zeta$ von $R_n(z)$ hei{\ss} t anziehend, absto{\ss} end oder indifferent, je nachdem $\vert R_n",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=216), last updated on Sun, 22 Nov 2009 11:41:33 +0100"
}

@Article{Frappier:IterationExponentials,
author="Frappier, Cl{\'e}ment",
title="Iterations of a kind of exponentials",
journal="Fibonacci Quart.",
year="1991",
volume="29",
number="4",
pages="351--361",
optkeywords="bibtex-import",
abstract="Let $0\square z=z$, $1\square z=z\sp z$ and $(n+1)\square z=(n\square z)\sp{n\square z}$ for $n=0,1,\ldots.$ Various elementary algebraic relationships are established as well as the limit $\lim\sb{n\to\infty}n\square e\sp{z/n}=1$ for complex $z$ with modulus less than $1/e$.\par The inverse function of $\zeta=n\square z$, denoted $z=(-n)\square \zeta$ for $n=1,2,\ldots$ ({\textquoteleft}{\textquoteleft}partial inverse{\textquoteright}{\textquoteright} see {\it R. A. Knoebel} [Am. Math. Mon. 88, 235-252 (1981; Zbl 0493.26007)]) is expressed in the form of an infinite product ($n$ a positive integer, $z$ complex such that $\vert\ln z\vert\le 1/ne)$. The author then considers extensions of the original definition of $n\square z$ to the rational, real, and finally complex $n$.\par For positive integers $q$ and $N(&gt;1)$ the functional equation $f\sb q(z)=z\sp N$, $f(0)=0$ is shown to have an analytic solution in a neighbourhood of the origin if and only if $N=M\sp q$ ($M$ a positive integer, $f\sb 1(z)=f(z)$, $f\sb{q+1}(z)=f(f\sb q(z)))$. Up to a multiplicative constant equal to an $\left({N-1\over M-1}\right)$-th root of unity, the solution is unique.",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=159), last updated on Sun, 22 Nov 2009 22:14:20 +0100",
opturl="http://eretrandre.org/rb/files/Frappier1991_159.pdf",
opturl=""
}

