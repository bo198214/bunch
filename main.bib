@Article{ref0644.30014,
author="Baker, I.N.
and Rippon, P.J.",
title="Towers of exponents and other composite maps",
journal="Complex Variables, Theory Appl.",
year="1989",
volume="12",
number="1-4",
pages="181--200",
optkeywords="iterates",
optkeywords="composite maps",
optkeywords="exponential map",
abstract="Given a region D and a sequence of maps $f\sb n: D\to D$ set $$\phi$ \sb n=f\sb 1\circ f\sb 2\circ...\circ f\sb n$. The authors give various conditions under which the sequence $$\phi$ \sb n$ is locally uniformly convergent in D to a constant limit. The case when D is the unit disc and all $f\sb n$ are the same is the well known theorem of Wolff and Denjoy. The theorems are applied to the special case of {\textquoteleft}{\textquoteleft}towers of exponents{\textquoteright}{\textquoteright}, that is the convergence of $$\phi$ \sb n(0)$ when each $$ f\sb n(z) = \exp (a\sb nz) $$ for some complex constant $a\sb n$. It is known that $$\phi$ \sb n(0)$ converges if all $a\sb n$ belong to the real segment [-e,1/e] [{\it D. F. Barrow}, Am. Math. Mon. 43, 150-160 (1936; Zbl 0013.25401)] or if all $a\sb n$ belong to the closed disc \=D(0,1/e) [{\it W. J. Thron}, Proc. Am. Math. Soc. 8, 1040-1043 (1958; Zbl 0081.057)]. The authors obtain some larger sets for which the sequence $$\phi$ \sb n(0)$ converges and also obtain some limitations on the sets for which convergence can occur.",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=234), last updated on Thu, 01 Apr 2010 23:40:32 +0200"
}

@Article{ref0705.33001,
author="Baker, I.N.
and Rippon, P.J.",
title="Iterating exponential functions with cyclic exponents",
journal="Math. Proc. Camb. Philos. Soc.",
year="1989",
volume="105",
number="2",
pages="357--375",
optkeywords="iterate",
optkeywords="exponent",
abstract="Let (1) $[a\sb 1,a\sb 2,...,a\sb n]=f\sb{a\sb 1}\circ f\sb{a\sb 2}\circ...\circ f\sb{a\sb n}$, where $a\sb i&gt;0$ and $f\sb a(x)=a\sp x$. Suppose that the (infinite) sequence $(a\sb n)$ has period k. It is shown that then (1) $[a\sb 1,a\sb 2,...,a\sb n]$ is a convergent sequence if and only if a unique representation of the numbers (2) $b\sb n=\log a\sb n$, $n=1,2,...,k$, in the form $b\sb 1=t\sb 1e\sp{-t\sb 2},b\sb 2=t\sb 2e\sp{-t\sb 3},...,b\sb k=t\sb ke\sp{-t\sb 1}$, where $\vert t\sb 1t\sb 2...t\sb k\vert \le 1$. This result is used to discuss the set of $(b\sb 1,...,b\sb k)$ in $R\sp k$ such that (1) is a convergent sequence, given (2). In particular, if $b\sb n\in [-e,e\sp{-1}]$, $1\le n\le k$, then (1) converges. The paper concludes with a proof of the result of {\it D. F. Barrow}, Am. Math. Mon. 43, 150-160 (1936; Zbl 0013.25401), that if $a\sb n=\exp (b\sb n)$ and $b\sb n$ is any (not necessarily periodic) sequence in $[-e,e\sp{-1}]$, then (1) converges.",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=254), last updated on Sun, 18 Apr 2010 23:29:25 +0200",
doi="10.1017/S0305004100067852"
}

