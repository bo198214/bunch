@Article{Walker:exponential,
author="Walker, Peter L.",
title="The exponential of iteration of $e\sp x-1$",
journal="Proc. Am. Math. Soc.",
year="1990",
volume="110",
number="3",
pages="611--620",
optkeywords="abel-equation",
optkeywords="abel-function",
optkeywords="asymptotic-solution",
optkeywords="decremented-exponential",
optkeywords="entire-solution",
abstract="Let an entire function $\phi (z)=z+\sum\sp{\infty}\sb{n=1}c\sb nz\sp{n+1}$ be given with $c\sb 1&gt;0$ and $c\sb n\ge 0$ for $n&gt;1$ and put $a=c\sb 1$, $b=(c\sb 2-c\sp 2\sb 1)/c\sb 1$. It has been proved in a previous paper of the author [Bull. Aust. Math. Soc. 38, 351-356 (1988; Zbl 0643.39002)] that if $b\ne 0$ and $c\sb 3&lt;c\sp 3\sb 1$, then for all $w\in {\bbfC}$ there exists the limit $$ \lim\sb{n\to \infty}\phi\sp n[1/(a(n-w)-b \log n)] $$ and it defines an entire nonconstant solution f of the equation $$ f(w+1)=\phi [f(w)]. $$ In the present paper the author studies properties of this function f in the case where $\phi (z)=e\sp z- 1$. In particular he locates its zeros and asymptotic values, shows that its derivative is never zero and for any $B&gt;0$ the set $\{$ z: Re f(z)$&gt;B\}$ has finite area (and, consequently, for any $A&gt;2$ the set $\{$ z: $\vert f(z)\vert &gt;A\}$ has finite area). The paper ends with some open question and a conjecture.[K.Baron]",
optlocation="Henryk Trappmann (henryk@pool.math.tu-berlin.de)",
optnote="exported from refbase (http://eretrandre.org/rb/show.php?record=90), last updated on Sun, 11 Oct 2009 10:12:14 +0200",
doi="10.2307/2047901",
opturl="http://www.zentralblatt-math.org/zmath/en/advanced/?q=an:0709.39007",
opturl="http://eretrandre.org/rb/files/Walker1990_90.pdf",
opturl=""
}

